    "string = 'abcdef,'\n",
    "def has_unique_chars(string):\n",
    "    log = dict()\n",
    "    for char in string:\n",
    "        log[char] = log.get(char,0) + 1\n",
    "        if log[char] > 1:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "has_unique_chars(string)"
